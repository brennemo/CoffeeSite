<div class="container">
    <!-- Example row of columns -->
    <div class="row">
		<h1>My Passport</h1> 
    </div>
    <div class="row">   
	    {{#each rows}}
	       <div class="col-xs-2">
	        	<a href="menu/{{this.shop_id}}">
	           	 <img src="{{this.shop_photo_t}}" alt="{{this.shop_name}}" class="img-responsive {{isVisited this.passport_visited}}"/>
	           </a>
	           <h4>{{this.shop_name}}</h4>
	           <p>{{passportDescription this.passport_date}}</p>
	           <button type="button" class="btn btn-link">Edit</button>
	           <button type="button" class="btn btn-link">Remove</button>
			</div><!--col-xs-2-->
	    {{/each}}
    </div><!--row-->
</div><!--container-->

