<div class="container">
    <div class="row">
		<h1>Explore</h1> 
    </div>
    <div class="row">
    	{{#each rows}}
	    	<div class="col-xs-6 col-sm-3">
		    	<div id="mycarousel" class="carousel slide" data-ride="carousel">
   					 <div class="carousel-inner">
       					 <div class="item active">
				    		<a href="menu/{{this.shop_id}}">
				    			<img src="{{this.shop_photo_s}}" alt="{{this.shop_name}}" class="img-responsive"/>
				    			<div class="carousel-caption"><h2 class="overlay-text">{{this.shop_name}}</h2></div>
				    		</a>
				    	</div>
				    </div>
		    	</div><!--explore-container-->
		    </div><!--col-xs-6 col-md-4-->
    	{{/each}}
    </div> <!--row-->
</div><!--container-->

